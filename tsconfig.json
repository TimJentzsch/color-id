{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$stores": ["src/stores"],
			"$stores/*": ["src/stores/*"],
			"$utils": ["src/utils"],
			"$utils/*": ["src/utils/*"],
			"$static": ["src/static"],
			"$static/*": ["src/static/*"]
		}
	}
}
